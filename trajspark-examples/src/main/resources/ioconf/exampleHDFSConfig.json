{
  "loadConfig": {
    "@type": "hdfs",
    "master": "local[*]",
    "location": "hdfs://localhost:9000/cars.csv",
    "fsDefaultName": "hdfs://localhost:9000",
    "fileMode": "single_file",
    "partNum": 8,
    "splitter": ","
  },
  "dataConfig": {
    "@type": "trajectory",
    "trajId": {
      "sourceName": "traj_id",
      "dataType": "String",
      "index": 1
    },
    "objectId": {
      "sourceName": "object_id",
      "dataType": "String",
      "index": 1
    },
    "trajPointConfig": {
      "@type": "traj_point",
      "pointId": {
        "sourceName": "point_id",
        "dataType": "String",
        "index": -1
      },
      "lng": {
        "sourceName": "lng",
        "dataType": "Double",
        "index": 3
      },
      "lat": {
        "sourceName": "lat",
        "dataType": "Double",
        "index": 2
      },
      "time": {
        "sourceName": "time",
        "dataType": "Date",
        "index": 4,
        "format": "yyyy-MM-dd HH:mm:ss"
      }
    }
  },
  "storeConfig": {
    "@type": "hdfs",
    "ip": "localhost",
    "port": "9000",
    "location": "hdfs://localhost:9000/test",
    "schema": "POINT_BASED_TRAJECTORY",
    "splitter": ",",
    "filePostFix": ".csv"
  }
}