{
  "loadConfig": {
    "@type": "hdfs",
    "master": "local[*]",
    "location": "hdfs://u0:9000/cars",
    "fsDefaultName": "hdfs://u0:9000",
    "fileMode": "multi_file",
    "partNum": 8,
    "splitter": ","
  },
  "dataConfig": {
    "@type": "trajectory",
    "trajId": {
      "sourceName": "traj_id",
      "dataType": "String",
      "index": 1
    },
    "objectId": {
      "sourceName": "object_id",
      "dataType": "String",
      "index": 1
    },
    "trajPointConfig": {
      "@type": "traj_point",
      "pointId": {
        "sourceName": "point_id",
        "dataType": "String",
        "index": -1
      },
      "lng": {
        "sourceName": "lng",
        "dataType": "Double",
        "index": 3
      },
      "lat": {
        "sourceName": "lat",
        "dataType": "Double",
        "index": 2
      },
      "time": {
        "sourceName": "time",
        "dataType": "Date",
        "index": 4,
        "format": "yyyy-MM-dd HH:mm:ss"
      }
    }
  },
  "storeConfig": {
    "@type": "hbase",
    "location": "hdfs://u0:9000/traj_geofence/",
    "schema": "POINT_BASED_TRAJECTORY",
    "dataSetName": "TRAJECTORY_GEOFENCE",
    "mainIndex": "OBJECT_ID_T",
    "otherIndex": "XZ2,TXZ2"
  }
}