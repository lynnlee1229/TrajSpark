{
  "loadConfig": {
    "@type": "standalone",
    "master": "local[*]",
    "location": "D:/gitRepo/TrajSpark/trajspark-examples/src/main/resources/data/geolife",
    "fileMode": "multi_file",
    "partNum": 8,
    "splitter": ","
  },
  "dataConfig": {
    "@type": "trajectory",
    "trajId": {
      "sourceName": "traj_id",
      "dataType": "String",
      "index": 1
    },
    "objectId": {
      "sourceName": "object_id",
      "dataType": "String",
      "index": 0
    },
    "trajPointConfig": {
      "@type": "traj_point",
      "pointId": {
        "sourceName": "point_id",
        "dataType": "String",
        "index": 2
      },
      "lng": {
        "sourceName": "lng",
        "dataType": "Double",
        "index": 4
      },
      "lat": {
        "sourceName": "lat",
        "dataType": "Double",
        "index": 3
      },
      "time": {
        "sourceName": "time",
        "dataType": "Date",
        "index": 5,
        "format": "yyyy-MM-dd HH:mm:ss"
      }
    }
  },
  "segmenterConfig": {
    "@type": "STAYPOINTBASED_SEGMENTER",
    "minTrajLength": 0.05,
    "detectorConfig": {
      "@type": "BASIC_DETECTOR",
      "maxStayDistInMeter": 900,
      "maxStayTimeInSecond": 900
    }
  },
  "storeConfig": {
    "@type": "hbase",
    "location": "hdfs://localhost:9000/store/",
    "schema": "POINT_BASED_TRAJECTORY",
    "dataSetName": "TRAJECTORY_TEST",
    "mainIndex": "XZ2",
    "otherIndex": "OBJECT_ID_T,TXZ2"
  }
}